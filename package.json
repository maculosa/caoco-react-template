{
  "name": "git_workflow",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "release": "node script/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "preinstall": "node scripts/preinstall.js"
  },
  "dependencies": {
    "dayjs": "^1.11.1",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.14.0",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "minimist": "^1.2.6",
    "prettier": "^2.6.2",
    "semver": "^7.3.7",
    "vite": "^2.9.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  }
}